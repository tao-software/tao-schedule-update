{
  "name": "tao-schedule-update",
  "version": "1.12.0",
  "description": "Allows you to plan changes on any post type",
  "main": "Gruntfile.js",
  "scripts": {
    "postinstall": "php node_modules/getcomposer/composer.phar install --no-dev && vendor/bin/phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs",
    "patch": "patch -p0 -ui phpcbf.diff"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tao-software/tao-schedule-update.git"
  },
  "keywords": [
    "publishing",
    "timing",
    "cron",
    "schedule",
    "unattended",
    "wordpress"
  ],
  "author": "TAO Digital",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tao-software/tao-schedule-update/issues"
  },
  "homepage": "https://github.com/tao-software/tao-schedule-update#readme",
  "devDependencies": {
    "getcomposer": "^1.3.3",
    "grunt": "^1.3.0",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-jscs": "^3.0.1",
    "grunt-phpcs": "^0.4.0"
  }
}
